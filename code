#include <stdio.h>

int
poslednaya ()
{
  int ls;
  scanf ("%d", &ls);
  return ls;
}

int
pervaya ()
{
  int fs;
  scanf ("%d", &fs);
  return fs;

}

int number = 0;

int
DigitCountGenius ()
{
  int result = 0;

  while (number != 0)
    {
      number /= 10;
      result++;
    }

  return result;
}

int
main (void)
{
  char chislo[50];
  int first, b, last, chl, chl1;
  printf ("first system: ");
  first = pervaya ();
  printf ("\nposledna'ya system: ");
  last = poslednaya ();
  printf ("vvedite chislo: ");
  scanf ("%d", &number);
  chl1 = number;
  int res = DigitCountGenius();
  for(int i = res; i>0; i--){
      chislo[i-1] = chl1 % 10;
      chl1/=10;
  }
  for(int i = 0; i<res; i++){
      printf("%d", chislo[i]);
  }
  return 0;
}
